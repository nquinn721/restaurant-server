{
  "name": "scheduling-backend",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "url": "https://bolt-schedular-backend.azurewebsites.net/",
  "license": "MIT",
  "scripts": {
    "deploy": "gcloud app deploy",
    "gcp-build": "npm run build",
    "prebuild": "rimraf dist",
    "build": "tsc -p tsconfig.build.json",
    "docs": "npx compodoc -p tsconfig.json -s",
    "format": "prettier --write \"apps/**/*.ts\" \"libs/**/*.ts\"",
    "start": "nest start",
    "start:dev": "NODE_ENV=develop nest start --watch",
    "start:test": "NODE_ENV=test nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "NODE_ENV=production node dist/apps/scheduling-backend/main",
    "lint": "tslint -p tsconfig.json -c tslint.json",
    "typeorm": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./apps/scheduling-backend/test/jest-e2e.json"
  },
  "dependencies": {
    "@nestjs/common": "^7.0.5",
    "@nestjs/core": "^7.0.5",
    "@nestjs/platform-express": "^7.0.5",
    "@nestjs/platform-socket.io": "^7.0.5",
    "@nestjs/swagger": "^4.4.0",
    "@nestjs/typeorm": "^7.0.0",
    "@nestjs/websockets": "^7.0.5",
    "@nestjsx/crud": "4.4.2",
    "@nestjsx/crud-typeorm": "4.4.2",
    "class-transformer": "^0.2.3",
    "class-validator": "^0.11.0",
    "helmet": "^3.21.2",
    "moment": "^2.25.3",
    "mysql": "^2.17.1",
    "nestjs-admin": "^0.4.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.0",
    "rxjs": "^6.5.3",
    "swagger-ui-express": "^4.1.2",
    "typeorm": "^0.2.24",
    "typeorm-encrypted-column": "^0.0.4"
  },
  "devDependencies": {
    "@compodoc/compodoc": "^1.1.11",
    "@nestjs/cli": "^7.0.2",
    "@nestjs/schematics": "^7.0.0",
    "@nestjs/testing": "^7.0.5",
    "@types/express": "^4.17.1",
    "@types/jest": "^24.0.18",
    "@types/mssql": "^6.0.0",
    "@types/node": "^12.7.5",
    "@types/passport-jwt": "^3.0.3",
    "@types/passport-local": "^1.0.33",
    "@types/socket.io": "^2.1.4",
    "@types/supertest": "^2.0.8",
    "jest": "^24.9.0",
    "prettier": "^1.18.2",
    "supertest": "^4.0.2",
    "ts-jest": "^24.1.0",
    "ts-loader": "^6.1.1",
    "ts-node": "^8.4.1",
    "tsconfig-paths": "^3.9.0",
    "tslint": "^5.20.0",
    "typescript": "^3.8.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": ".",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "./coverage",
    "testEnvironment": "node",
    "roots": [
      "<rootDir>/apps/"
    ]
  }
}
